{
  "name": "admin-console-server",
  "version": "1.0.0",
  "description": "Express.js server for Admin Console with public and private APIs",
  "main": "src/index.js",
  "scripts": {
    "start:dev": "nodemon src/index.js",
    "start:bundle": "nodemon build/bundle.js",
    "start:prod": "node build/bundle.js",
    "build": "webpack --mode development",
    "sample": "node ./src/samples/loadSampleData.js",
    "blowitallaway": "node ./src/samples/loadSampleData.js --delete"
  },
  "keywords": [
    "Express.js",
    "Mongoose",
    "Passport.js",
    "MongoDB"
  ],
  "author": "Alexandr Ketov <ketov.alex.v@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "mongoose": "^5.4.16",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "nodemon": "^1.18.10",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  }
}
